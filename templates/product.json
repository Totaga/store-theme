{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "caption": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "dropdown",
            "swatch_shape": "circle"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true,
            "show_gift_card_recipient": true
          }
        },
        "collapsible-row-1": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Shipping & Returns",
            "icon": "truck",
            "content": "",
            "page": ""
          }
        },
        "custom_liquid_bxfykh": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% if product.metafields.custom.device_type %}\n  <div class=\"product-device-type\">\n    <span>Device Type: {{ product.metafields.custom.device_type.value.name }}<\/span>\n    {% if product.metafields.custom.device_type.value.icon %}\n      <iconify-icon icon=\"mdi:{{ product.metafields.custom.device_type.value.icon }}\" ><\/iconify-icon><\/span>\n    {% endif %}\n  <\/div>\n{% endif %}"
          }
        },
        "custom_liquid_VcAAC6": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% if product.metafields.custom.ecosystems.value %}\n  <div class=\"product-ecosystems\">\n    <ul>\n      {% assign ecosystems = product.metafields.custom.ecosystems.value | json_parse %}\n      {% for ecosystem in ecosystems %}\n        <li>\n          <div class=\"ecosystem\">{{ ecosystem.name }}\n          {% if ecosystem.icon%}\n            {{ ecosystem.icon | img_tag }}\n          {% endif %}\n          <\/div>\n        <\/li>\n      {% endfor %}\n    <\/ul>\n  <\/div>\n{% endif %}\n<pre>{{ product.metafields.custom.ecosystems.value | json }}<\/pre>"
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "collapsible_tab_BU9it8": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Ecosystems",
            "icon": "lightning_bolt",
            "content": "{{ product.metafields.custom.ecosystems | metafield_tag: field: 'name' }}<p><\/p>",
            "page": ""
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "caption",
        "price",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "collapsible-row-1",
        "custom_liquid_bxfykh",
        "custom_liquid_VcAAC6",
        "share",
        "description",
        "collapsible_tab_BU9it8"
      ],
      "settings": {
        "enable_sticky_info": true,
        "color_scheme": "",
        "media_size": "small",
        "constrain_to_viewport": true,
        "media_fit": "cover",
        "gallery_layout": "thumbnail_slider",
        "media_position": "left",
        "image_zoom": "lightbox",
        "mobile_thumbnails": "hide",
        "hide_variants": true,
        "enable_video_looping": false,
        "padding_top": 36,
        "padding_bottom": 12
      }
    },
    "related-products": {
      "type": "related-products",
      "settings": {
        "heading": "You may also like",
        "heading_size": "h2",
        "products_to_show": 4,
        "columns_desktop": 4,
        "color_scheme": "scheme-1",
        "image_ratio": "square",
        "image_shape": "default",
        "show_secondary_image": true,
        "show_vendor": false,
        "show_rating": false,
        "columns_mobile": "2",
        "padding_top": 36,
        "padding_bottom": 28
      }
    },
    "1713982866c7aeb98f": {
      "type": "apps",
      "settings": {
        "include_margins": true
      }
    }
  },
  "order": [
    "main",
    "related-products",
    "1713982866c7aeb98f"
  ]
}
