{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "caption": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "dropdown",
            "swatch_shape": "circle"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true,
            "show_gift_card_recipient": true
          }
        },
        "collapsible-row-1": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Shipping & Returns",
            "icon": "truck",
            "content": "",
            "page": ""
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "collapsible_tab_BU9it8": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Integrations",
            "icon": "lightning_bolt",
            "content": "{{ product.metafields.custom.ecosystems | metafield_tag: field: 'name' }}<p><\/p>",
            "page": ""
          }
        },
        "custom_liquid_fmcMYf": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<div class=\"product-ecosystems\">\n  {% assign ecosystems = product.metafields.custom.ecosystems | json_parse %}\n  <ul style=\"display: flex; list-style: none; padding: 0;\">\n    {% for ecosystem in ecosystems %}\n      {% if ecosystem.icon and ecosystem.website %}\n        <li style=\"margin-right: 10px;\">\n          <a href=\"{{ ecosystem.website }}\" target=\"_blank\" aria-label=\"Ecosystem Link\">\n            <img src=\"{{ ecosystem.icon | img_url: 'original' }}\" alt=\"Ecosystem Icon\" style=\"height: 30px; width: auto;\">\n          <\/a>\n        <\/li>\n      {% endif %}\n    {% endfor %}\n  <\/ul>\n<\/div>"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "caption",
        "price",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "collapsible-row-1",
        "share",
        "description",
        "collapsible_tab_BU9it8",
        "custom_liquid_fmcMYf"
      ],
      "settings": {
        "enable_sticky_info": true,
        "color_scheme": "",
        "media_size": "small",
        "constrain_to_viewport": true,
        "media_fit": "cover",
        "gallery_layout": "thumbnail_slider",
        "media_position": "left",
        "image_zoom": "lightbox",
        "mobile_thumbnails": "hide",
        "hide_variants": true,
        "enable_video_looping": false,
        "padding_top": 36,
        "padding_bottom": 12
      }
    },
    "related-products": {
      "type": "related-products",
      "settings": {
        "heading": "You may also like",
        "heading_size": "h2",
        "products_to_show": 4,
        "columns_desktop": 4,
        "color_scheme": "scheme-1",
        "image_ratio": "square",
        "image_shape": "default",
        "show_secondary_image": true,
        "show_vendor": false,
        "show_rating": false,
        "columns_mobile": "2",
        "padding_top": 36,
        "padding_bottom": 28
      }
    }
  },
  "order": [
    "main",
    "related-products"
  ]
}
